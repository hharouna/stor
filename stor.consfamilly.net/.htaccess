RewriteEngine on


#Lien d'activation 
RewriteRule ^active/0/([0-9]+)/([0-9a-zA-Z@_.]+)/([0-9a-zA-Z@_.]+)/([0-9a-zA-Z@_.]+)  contenu/functionphp/activation.php?idincs=$1&pseudo=$2&email=$3&ssl=$4   [L]
RewriteRule ^mdp/0/([0-9]+)/([0-9]+)/([0-9]+) contenu/functionphp/mdp.php?idincs=$1&temps=$2&idcontrole=$3   [L]
RewriteRule ^mdp/0/mdpcontrole?$ contenu/functionphp/mdpcontrole.php  [L]
RewriteRule ^valider/mdp?$ contenu/functionphp/validermdp.php  [L]
RewriteRule ^inscription/freelance?$ contenu/forconnexion/forinscription.php  [L]


RewriteRule ^url?$ index.php [L]

RewriteRule ^url/controle?$ url/controle.php [L]	

RewriteRule ^connexion/accueil?$ contenu/accueil/accueil.php [L]	

RewriteRule ^erreur/401?$ contenu/erreur/404.php [L]
RewriteRule ^erreur/402?$ contenu/erreur/404.php [L]
RewriteRule ^/erreur/403?$ contenu/erreur/404.php [L]
RewriteRule ^erreur/404?$ contenu/erreur/404.php [L]

#ensemble des urls  des  erreurs -- consfamilly --
ErrorDocument 401 https://consfamilly.net/erreur/401
ErrorDocument 402 https://consfamilly.net/erreur/402
ErrorDocument 403 /erreur/403
ErrorDocument 404 https://consfamilly.net/erreur/404
